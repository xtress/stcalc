<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            
            <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.css') }}" type="text/css" media="all" />
            
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body style="width:960px; text-align: center">
        {% block body %}{% endblock %}
        {% block javascripts %}
            
            <script type="text/javascript" src="{{ asset('steamcalc/js/jquery-ui-1.9.2.custom.min.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('bootstrap/js/bootstrap.js') }}"></script>
            
        {% endblock %}
    </body>
</html>
